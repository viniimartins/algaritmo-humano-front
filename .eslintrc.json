{
  "extends": [
    "@rocketseat/eslint-config/next",
    "next/core-web-vitals",
    "next/typescript"
  ],
  "plugins": [
    "simple-import-sort"
  ],
  "rules": {
    "@typescript-eslint/no-namespace": "off",
    "simple-import-sort/imports": "error",
    "react-hooks/rules-of-hooks": "off",
    "camelcase": "off",
    "no-restricted-syntax": "off",
    "no-case-declarations": "off"
  }
}